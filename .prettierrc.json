{
  "useTabs": false,
  "tabWidth": 2,
  "singleQuote": true,
  "semi": true,
  "overrides": [
    {
      "files": "*.json",
      "options": {
        "printWidth": 200
      }
    }
  ],
  "proseWrap": "preserve",
  "htmlWhitespaceSensitivity": "strict",
  "arrowParens": "always",
  "endOfLine": "auto"
}
